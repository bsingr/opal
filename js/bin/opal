#!/usr/bin/env narwhal

var is_node = (typeof process !== 'undefined');
var switches, program_file, opal_argv;

if (is_node) {
  throw "need to implement bin for node";
}
else {
  switches = [];
  program_file = system.args[1] || "";
  opal_argv = system.args.slice(2);
}

require('../lib_js/opal').run(switches, program_file, opal_argv);
